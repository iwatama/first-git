<h1>messageList</h1>
<ul>
    <table border = 1>
        <tr>
            <td>id</td>
            <td>title</td>
            <td>message</td>
        </tr>
        <% @messages.each do |message| %>
            <tr>
                <td><%= link_to message.id, message %></td>
                <td><%= message.title %></td>
                <td><%= message.content %></td>
            </tr>
        <% end %>
    </table>
</ul>

<%= paginate @messages %>
<%= link_to '新規メッセージの作成', new_message_path %>


